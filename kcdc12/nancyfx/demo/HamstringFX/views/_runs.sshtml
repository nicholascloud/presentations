<!-- _runs.sshtml -->
@Each.Runs
<div class="run">
  <h2>@Current.ScheduledAt</h2>
  <dl>
    <dt>Your time:</dt><dd>@Current.Duration</dd>
    <dt>Best time:</dt><dd>00:00:00</dd>
    <dt>Difference:</dt><dd>00:00:00</dd>
  </dl>
  <p>@Current.Distance miles</p>
  <p>@Current.RouteName</p>
</div>
@EndEach