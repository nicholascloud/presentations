<!-- _runs.sshtml -->
@Each.Runs
<div class="run">
  <h2>@Current.When</h2>
  <dl>
    <dt>Your time:</dt><dd>@Current.YourTime</dd>
    <dt>Best route time:</dt><dd>@Current.BestRouteTime</dd>
    <dt>Difference:</dt><dd>@Current.Difference</dd>
  </dl>
  <p>@Current.Distance</p>
  <p>@Current.Where</p>
</div>
@EndEach